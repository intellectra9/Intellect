    },
    {
      "id": "i93",
      "number": 93,
      "sentence": "not for her validation.",
      "start": 256.78,
      "end": 257.822,
      "duration": 1.042
    },
    {
      "id": "i94",
      "number": 94,
      "sentence": "Person A sends: \"Hey, how was your day? Hope you had a good one!\"",
      "start": 258.302,
      "end": 260.302,
      "duration": 2.0
    },
